{
  "name": "itm",
  "version": "0.1.0-alpha.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "core:doc": "typedoc --out dist/doc projects/itm-core/src --theme doc",
    "demo:monaco:clean": "rimraf projects/itm-demo/tmp/monaco && mkdir -p projects/itm-demo/tmp/monaco",
    "demo:monaco:compile": "webpack-cli --config projects/itm-demo/monaco.webpack.config.js",
    "demo:monaco:build": "npm run demo:monaco:clean && npm run demo:monaco:compile",
    "demo:compile": "ng build itm-demo",
    "demo:serve": "ng serve itm-demo",
    "demo:dev": "npm run demo:monaco:build && npm run demo:serve",
    "demo:build": "npm run demo:monaco:build && npm run demo:compile"
  },
  "private": true,
